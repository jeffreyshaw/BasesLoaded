            HorizontalAlignment="Center" 
			Height="5" 
			Margin="0,499,0,-1" 
			VerticalAlignment="Bottom" 
			Width="402"/>
        <Label Content="* = Required" 
			HorizontalAlignment="Center" 
			Margin="10,468,305,6" 
			VerticalAlignment="Center" 
			Height="26"/>
    </Grid>
</Window>

        	Content="*" 
        	HorizontalAlignment="Center" 
        	Margin="260,430,124,47" 
        	VerticalAlignment="Center" 
        	Height="23" 
        	RenderTransformOrigin="5.2,4.087"/>
        <PasswordBox x:Name="Confirm_Password_Input" 
                     HorizontalAlignment="Center" 
                     Height="26" 
                     Margin="130,427,150,43" 
                     VerticalAlignment="Center" 
                     Width="120"/>

        	Content="New Password" 
        	HorizontalAlignment="Center" 
        	Margin="6,400,297,73" 
        	VerticalAlignment="Center" 
        	Height="27" 
        	Width="97"/>
        <Label x:Name="New_Password_Required" 
        	Content="*" 
        	HorizontalAlignment="Center" 
